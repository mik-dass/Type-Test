{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "module": "commonjs",
    "strict": false,
    "outDir": "./build",
    "sourceMap": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "assets",
    "node_modules",
    "bower_components",
    "jspm_packages",
    "build",
    "testing_fun"
  ],
  "typeAcquisition": {
    "enable": true
  }
}